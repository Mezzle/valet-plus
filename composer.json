    "require": {
        "php": ">=7.2",
        "illuminate/container": "~6.8",
        "mnapoli/silly": "~1.7.2",
        "nategood/httpful": "~0.3",
        "sebastian/version": "^2.0",
        "squizlabs/php_codesniffer": "*",
        "symfony/process": "~2.7 || ~3.0",
        "tightenco/collect": "~5.4.0"
    },
    "replace": {
        "laravel/valet": "*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8"
    },
    "config": {
        "platform": {
            "php": "7.2.24"
        }
    },
    "autoload": {
        "psr-4": {
            "Valet\\": "cli/Valet/"
        },
        "files": [
            "cli/includes/compatibility.php",
            "cli/includes/facades.php",
            "cli/includes/helpers.php"
        ]
    },
    "bin": [
        "valet"
    ]
}
